public class reverse{
    public static void main(String args[]){
        int n = 1123;
        rev(n);
    }
    public static void rev(int number){
        int last;
        int re =0;
        while(number >0){
            last = number % 10;
            re = re * 10 + last;
            number = number / 10;

        }
        
        System.out.println(re);

    }
}
