

import java.util.Scanner;

public class oneToN_Back {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        fun(n);
        
    }
    public static void fun(int n ){
        if(n<1) return;
        fun(n-1);
        System.out.println(n);
    }
}
