import java.util.Scanner;

public class oneton {
    public static void main(String[] args) {
        int n ;
        Scanner s = new Scanner(System.in);
        n = s.nextInt();
        f(1, n);

    }
       
    

public static void f(int i , int n ){
    if(i>n) return ;
    System.out.println(i);
    i++;
    f(i,n);
}
}
