import java.util.Scanner;

public class nToOne {
    public static void main(String[] args) {
        int n;
        Scanner s = new Scanner(System.in);
        n = s.nextInt();
        fun (n);
    }
    public static void fun(int n ){
        if(n<1) return ;
        System.out.println(n);
        fun(--n);

    }
    
}
