// Here is the code which will remove the vowels in a string and print the consonent string

public class vowel {
    public static void main(String args[]){
        StringBuilder ans = new StringBuilder();
        String sa = "howareyou";
        for(int i =0; i<sa.length(); i++){
            char c = sa.charAt(i);
            if(c =='a'|| c=='e' || c=='i' || c=='o' || c=='u'){
                continue;
            
            }
            ans.append(c);

        }
        System.err.println(ans);
    }
    
}
